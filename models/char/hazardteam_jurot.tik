	//normal +use
//	cache localization/sound/dialog/combat/jurot_moveon.mp3
//	cache localization/sound/dialog/combat/jurot_whatisal.mp3
//	cache localization/sound/dialog/combat/jurot_whatisit.mp3
//	cache localization/sound/dialog/combat/jurot_sir9.mp3
//	cache localization/sound/dialog/combat/jurot_canihelpu.mp3
//	cache localization/sound/dialog/combat/jurot_alex.mp3

	useactorweapon "CompressionRifleEnhanced"
	}
	client
	{
		aliascache snddeath1 localization/sound/dialog/combat/jurot_collpain.mp3
        aliascache snddeath2 localization/sound/dialog/combat/jurot_longp.mp3
		aliascache sndroll sound/chars/human/human_roll.wav
		aliascache sndland sound/misc/land.wav
	}
}

